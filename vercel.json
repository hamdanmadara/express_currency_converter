{
    "version": 2,
    "builds": [
      {
        "src": "dist/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "dist/server.js"
      },
      {
        "src": "/health",
        "dest": "dist/server.js"
      }
    ],
    "env": {
      "PORT": "3000",
      "NODE_ENV": "production"
    }
  }